block cssList
    link(rel='stylesheet', href='/stylesheets/examples/electronic-world/eworld.css')
block jsList
    script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js", type="text/javascript")
    script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.3.8/angular.js', type='text/javascript')

    script(src='/javascripts/examples/electronic-world/app.js', type='text/javascript')
    script(src='/javascripts/examples/electronic-world/map/map.js', type='text/javascript')
    script(src='/javascripts/examples/electronic-world/map/MapController.js', type='text/javascript')
    script(src='/javascripts/examples/electronic-world/map/MapCreatorService.js', type='text/javascript')

block content
    h1= title
    div#display(ng-app="eWorld", ng-controller="MapController")
        pre(ng-bind="world")
        div
            p {{info.name}} - {{info.version}}



